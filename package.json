{
	"name": "rollup-plugin-javascript-confuser",
	"version": "1.0.3",
	"description": "A Rollup Plugin for js-confuser",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"typesVersions": {
		"*": {
			"import": [
				"./dist/index.d.mts"
			],
			"require": [
				"./dist/index.d.ts"
			]
		}
	},
	"exports": {
		".": {
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			}
		}
	},
	"scripts": {
		"prepublish": "tsup --config tsup.config.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/EXPress-016/rollup-plugin-javascript-confuser.git"
	},
	"keywords": [
		"rollup",
		"vite",
		"obfuscator",
		"obfuscation",
		"uglify",
		"crush",
		"code protection",
		"javascript obfuscator",
		"js obfuscator",
		"js confuser",
		"javascript confuser"
	],
	"author": "EXPress-016",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/EXPress-016/rollup-plugin-javascript-confuser/issues"
	},
	"homepage": "https://github.com/EXPress-016/rollup-plugin-javascript-confuser#readme",
	"dependencies": {
		"anymatch": "^3.1.3"
	},
	"peerDependencies": {
		"js-confuser": "^2.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.0.6",
		"@types/node": "^22.16.0",
		"rollup": "^4.44.2",
		"tsup": "^8.5.0",
		"typescript": "^5.8.3"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"packageManager": "pnpm@10.11.0"
}
